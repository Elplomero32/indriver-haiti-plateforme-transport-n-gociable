# API Keys
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_SOCKET_URL=http://localhost:3000

# App Configuration
VITE_APP_NAME=InDriver Haiti
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_SMS_NOTIFICATIONS=true

# API Endpoints
VITE_API_URL=http://localhost:3000/api
VITE_WEBSOCKET_URL=ws://localhost:3000

# Authentication
VITE_JWT_EXPIRY=24h
VITE_REFRESH_TOKEN_EXPIRY=7d

# Map Configuration
VITE_DEFAULT_LAT=18.9712
VITE_DEFAULT_LNG=-72.2852
VITE_DEFAULT_ZOOM=13

# Commission Rates (in percentage)
VITE_PLATFORM_COMMISSION=10

# Currency
VITE_DEFAULT_CURRENCY=HTG
VITE_CURRENCY_SYMBOL=HTG

# Timeouts (in milliseconds)
VITE_REQUEST_TIMEOUT=30000
VITE_SOCKET_TIMEOUT=5000

# Debug Mode
VITE_DEBUG_MODE=true
